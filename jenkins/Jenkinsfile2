pipeline{
 agent any;
 tools{
     maven 'M3'
    
 }
  stages {
    stage('Maven-Clean'){
      steps{
        sh 'mvn clean'
      }
    }
    stage('Maven-Compile'){
      steps{
        sh 'mvn compile'
      }
    }
    stage('Maven-Test'){
      steps{
        sh 'mvn test'
      }
    }
    stage('Maven-Package'){
        steps{
        sh 'mvn package'
        }
    }
    
    stage("build & SonarQube analysis") {
            steps {
              withSonarQubeEnv('sonar-server-local') {
                sh 'mvn sonar:sonar'
            }
        }
    }
    }
